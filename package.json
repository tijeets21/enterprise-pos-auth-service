{
  "name": "projectv-middleware",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Express middleware API for MongoDB management",
  "scripts": {
    "dev": "node src/index.js",
    "start": "cross-env NODE_ENV=production node src/index.js",
